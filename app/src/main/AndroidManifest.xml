<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.pitt.cs.cs1635.openclicker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".student.StudentLoginActivity" />
        <activity android:name=".student.StudentClassListActivity" />
        <activity android:name=".student.WaitForQuestionActivity" />
        <activity android:name=".student.AnswerQuestionActivity" />
        <activity android:name=".student.EnrollActivity" />
        <activity android:name=".teacher.TeacherLoginActivity" />
        <activity android:name=".teacher.TeacherClassListActivity" />
        <activity android:name=".teacher.CreateClassActivity" />
        <activity android:name=".teacher.ClassInfoActivity" />
        <activity android:name=".teacher.CreateQuestionActivity" />
        <activity android:name=".teacher.AskQuestion"></activity>
    </application>

</manifest>
